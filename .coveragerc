[run]
source = src
omit =
    */tests/*
    */__init__.py
    src/main.py
    src/detector.py
    src/face_auth.py
    src/notifier.py
    src/config.py

[report]
show_missing = True
precision = 2
exclude_lines =
    pragma: no cover
    def __repr__
    if __name__ == .__main__.:
    raise NotImplementedError
